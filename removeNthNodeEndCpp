class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        int count = 1;
    	ListNode* temp = head;
    	while(temp!=NULL) {
        	count++;
        	temp = temp->next;
    	}
   	 
    	temp = head;
   	 
    	for(int i = 0; i < count-n+1 ; i++) {
        		temp = temp->next;
    	}
        delete (temp);
   	    return head;
        
    }
};
